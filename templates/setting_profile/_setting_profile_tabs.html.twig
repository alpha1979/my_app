{% embed "shared/_tabs.html.twig" %}
    {% block tabs %}
        {% embed "shared/_tab.html.twig" with { route: 'app_setting_profile' } %}
            {% block content %}
                Your profile setting
            {% endblock %}
            {% block path %}
                {{ path('app_setting_profile')}}
            {% endblock %}
        {% endembed %}
       
        {% embed "shared/_tab.html.twig" with { route: 'app_setting_profile_image', classes:'ml-4' } %}
            {% block content %}
                Your profile image           
            {% endblock %}
            {% block path %}
                {{ path('app_setting_profile_image')}}
            {% endblock %}
        {% endembed %}
    {% endblock %}
{% endembed %} 